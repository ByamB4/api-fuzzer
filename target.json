      "params": {
        "password": {
          "requirements": []
        },
        "phone": {
          "requirements": {
            "min_length": 12,
            "max_length": 12
          }
        },
        "deviceId": {
          "requirements": []
        }
      }
    },
    {
      "endpoint": "/api/integration/loan-application/payon/offers",
      "methods": ["GET"],
      "params": {
        "productId": {
          "requirements": []
        }
      }
    }
  ]
}
